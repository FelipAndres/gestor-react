const handleSubmitPut = (event) => {
  event.preventDefault()
  doFetch({
    method: 'post',
    data: objProducto
  })
  // validar si ocurre un error o no con el destructuring de useFetch
  if (isLoading) {
    console.log('esperando el registro')
  } else {
    if (error) {
      console.error(error)
    } else {
      setProducto(objProducto)
      setIsOpen(!isOpen)
      toast.success('Registrado con exito')
    }
  }
}
const handleSubmitDel = (event) => {
  event.preventDefault()
  doFetch({
    method: 'post',
    data: objProducto
  })
  // validar si ocurre un error o no con el destructuring de useFetch
  if (isLoading) {
    console.log('esperando el registro')
  } else {
    if (error) {
      console.error(error)
    } else {
      setProducto(objProducto)
      setIsOpen(!isOpen)
      toast.success('Registrado con exito')
    }
  }
}

if (isLoading) {
  console.log('esperando el registro')
} else {
}
